   3.664937677568424*^9, {3.664937777346137*^9, 3.66493778112724*^9}}],

Cell["\<\
The growth timescale is taken to be the slope of the best fit line to the \
above log plots\
\>", "Text",
 CellChangeTimes->{{3.6649376809409738`*^9, 3.6649377037276216`*^9}, {
  3.6649377662684493`*^9, 3.664937771839739*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Column", "[", 
  RowBox[{"Row", "/@", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\"\< k = \>\"", "<>", 
          RowBox[{"ToString", "@", "k"}]}], ",", 
         RowBox[{"\"\<  np/ng = \>\"", "<>", 
          RowBox[{"ToString", "@", "r"}]}], ",", 
         RowBox[{"\"\<  k*v0/wp = \>\"", "<>", 
          RowBox[{"ToString", "[", 
           RowBox[{"k", " ", 
            RowBox[{"4", "/", 
             RowBox[{"wp", "[", "r", "]"}]}]}], "]"}]}], ",", 
         RowBox[{"\"\<  growth timescale = \>\"", "<>", 
          RowBox[{"ToString", "@", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"w", "=", 
               RowBox[{"NestList", "[", 
                RowBox[{
                 RowBox[{"H", "[", "r", "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"xF", "[", 
                    RowBox[{"r", ",", "k"}], "]"}], ",", 
                   RowBox[{"vF", "[", "r", "]"}]}], "}"}], ",", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"T", "[", "r", "]"}], "/", "dt"}], "//", 
                  "Floor"}]}], "]"}]}], "}"}], ",", 
             RowBox[{
              RowBox[{"FindFit", "[", 
               RowBox[{
                RowBox[{"Transpose", "[", 
                 RowBox[{"{", 
