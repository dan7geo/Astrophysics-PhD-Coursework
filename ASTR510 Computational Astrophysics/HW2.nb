                   RowBox[{"Range", "[", 
                    RowBox[{"0", ",", 
                    RowBox[{"T", "[", "r", "]"}], ",", "dt"}], "]"}], ",", 
                   RowBox[{"Log", "[", 
                    RowBox[{"Max", "/@", 
                    RowBox[{"Abs", "@", 
                    RowBox[{"w", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", ",", "2"}], "]"}], "]"}]}]}], "]"}]}], 
                  "}"}], "]"}], ",", 
                RowBox[{
