Cell["\<\
Thus we can write the hamiltonian in terms of PA, PB, PR, A, B, and R as\
\>", "Text",
 CellChangeTimes->{{3.6504126677898755`*^9, 3.6504127270382643`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"H", "=", 
  RowBox[{
   RowBox[{"-", 
    FractionBox[
     RowBox[{"G", " ", "m1", " ", "m2"}], 
     RowBox[{"R", "[", "t", "]"}]]}], "+", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", " ", "m1", " ", "m2"}]], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"PR", "[", "t", "]"}], "^", "2"}], " ", "+", 
      RowBox[{
       RowBox[{
        RowBox[{"PA", "[", "t", "]"}], "^", "2"}], "/", 
       RowBox[{
        RowBox[{"R", "[", "t", "]"}], "^", "2"}]}], "+", 
      RowBox[{
       RowBox[{
        RowBox[{"PB", "[", "t", "]"}], "^", "2"}], "/", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"R", "[", "t", "]"}], "^", "2"}], " ", 
         RowBox[{
          RowBox[{"Sin", "[", 
           RowBox[{"A", "[", "t", "]"}], "]"}], "^", "2"}]}], ")"}]}]}], 
     ")"}], 
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.650412856482668*^9, 3.6504129591825423`*^9}, {
   3.650412991576395*^9, 3.650412995263606*^9}, {3.650413031277666*^9, 
   3.650413040509194*^9}, {3.6504132160192327`*^9, 3.650413276629699*^9}, {
   3.6504133550801864`*^9, 3.650413408219226*^9}, 3.6504134540768485`*^9}],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"PR", "[", "t", "]"}], "2"], "+", 
      FractionBox[
       SuperscriptBox[
        RowBox[{"PA", "[", "t", "]"}], "2"], 
       SuperscriptBox[
        RowBox[{"R", "[", "t", "]"}], "2"]], "+", 
      FractionBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"Csc", "[", 
          RowBox[{"A", "[", "t", "]"}], "]"}], "2"], " ", 
        SuperscriptBox[
         RowBox[{"PB", "[", "t", "]"}], "2"]}], 
       SuperscriptBox[
        RowBox[{"R", "[", "t", "]"}], "2"]]}], ")"}]}], 
   RowBox[{"2", " ", "m1", " ", "m2"}]], "-", 
  FractionBox[
   RowBox[{"G", " ", "m1", " ", "m2"}], 
   RowBox[{"R", "[", "t", "]"}]]}]], "Output",
 CellChangeTimes->{
  3.6504132884473753`*^9, {3.6504133570673*^9, 3.650413408677252*^9}, 
   3.6504134586131077`*^9, 3.6504591201256027`*^9, 3.650459475364064*^9}]
}, Open  ]],

