 RowBox[{
  RowBox[{"D", "[", 
   RowBox[{
    RowBox[{"PA", "[", "t", "]"}], ",", "t"}], "]"}], "==", 
  RowBox[{"-", 
   RowBox[{"D", "[", 
    RowBox[{"H", ",", 
     RowBox[{"A", "[", "t", "]"}]}], "]"}]}], "\[Equal]", "0"}]], "Input",
 CellChangeTimes->{{3.650410628017223*^9, 3.6504106384518166`*^9}, {
