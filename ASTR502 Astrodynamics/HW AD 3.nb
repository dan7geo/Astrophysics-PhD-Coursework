Notebook[{

Cell[CellGroupData[{
Cell["Problem 2", "Chapter",
 CellChangeTimes->{{3.650318608885084*^9, 3.650318618500608*^9}}],

Cell[CellGroupData[{

Cell["a)", "Subchapter",
 CellChangeTimes->{{3.6504101372541556`*^9, 3.650410158742385*^9}}],

Cell["\<\
Postion of first mass = x1, Postion of second mass = x2, Vector connecting \
the two masses = r, Position of center of mass = X. (Note that x1, x2, r, X \
are all vectors)\
\>", "Text",
 CellChangeTimes->{{3.650407728485406*^9, 3.6504077942851696`*^9}, {
  3.650407883404267*^9, 3.65040790583555*^9}, {3.650408499622505*^9, 
  3.650408501677623*^9}, {3.650408691516478*^9, 3.6504086932685785`*^9}}],

Cell["Equation of position of center of mass:", "Text",
 CellChangeTimes->{{3.6504531085617514`*^9, 3.6504531178492827`*^9}, {
  3.650459003475775*^9, 3.650459005755379*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"e1", "=", 
  RowBox[{
   RowBox[{"X", "[", "t", "]"}], "==", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"m1", " ", 
       RowBox[{"x1", "[", "t", "]"}]}], "+", 
      RowBox[{"m2", " ", 
       RowBox[{"x2", "[", "t", "]"}]}]}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.650407808727996*^9, 3.6504078297101955`*^9}, {
   3.650407867189339*^9, 3.6504078690444455`*^9}, 3.650407964164883*^9, {
   3.6504080227002316`*^9, 3.650408024892357*^9}, {3.6504082343223324`*^9, 
   3.650408247930111*^9}, {3.6504087176699743`*^9, 3.6504087191330576`*^9}, 
   3.650459012915593*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"X", "[", "t", "]"}], "\[Equal]", 
  FractionBox[
   RowBox[{
    RowBox[{"m1", " ", 
     RowBox[{"x1", "[", "t", "]"}]}], "+", 
    RowBox[{"m2", " ", 
     RowBox[{"x2", "[", "t", "]"}]}]}], 
   RowBox[{"m1", "+", "m2"}]]}]], "Output",
 CellChangeTimes->{
  3.6504078308972635`*^9, 3.6504079089677286`*^9, 3.650407964469901*^9, 
   3.6504081553138137`*^9, 3.6504082666421814`*^9, {3.6504087250933986`*^9, 
   3.6504087343419275`*^9}, {3.650411019472607*^9, 3.6504110426089306`*^9}, 
   3.6504549017047634`*^9, {3.650459008749386*^9, 3.650459032516431*^9}, 
   3.650459474989664*^9}]
}, Open  ]],

Cell["Equation of vector from one body to the other:", "Text",
 CellChangeTimes->{{3.650453123464604*^9, 3.6504531388964863`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"e2", "=", 
  RowBox[{
   RowBox[{"r", "[", "t", "]"}], "==", 
   RowBox[{
    RowBox[{"x2", "[", "t", "]"}], "-", 
    RowBox[{"x1", "[", "t", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.6504079151380787`*^9, 3.6504079187562857`*^9}, {
  3.650407981428871*^9, 3.6504079834519863`*^9}, {3.650408237683525*^9, 
  3.65040824231479*^9}, {3.6504087215901985`*^9, 3.650408722797267*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"r", "[", "t", "]"}], "\[Equal]", 
  RowBox[{
   RowBox[{"-", 
    RowBox[{"x1", "[", "t", "]"}]}], "+", 
   RowBox[{"x2", "[", "t", "]"}]}]}]], "Output",
 CellChangeTimes->{
  3.650407919889351*^9, 3.650407983832008*^9, 3.650408157126917*^9, 
   3.650408268428283*^9, 3.650408735989022*^9, {3.650411029555184*^9, 
   3.6504110444920387`*^9}, 3.6504549035485663`*^9, 3.6504590342636337`*^9, 
   3.650459475052064*^9}]
}, Open  ]],

Cell["Solving for x1 and x2 in terms of X and r:", "Text",
 CellChangeTimes->{{3.6504084708458595`*^9, 3.650408487445809*^9}, 
   3.6504145006847115`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"s", "=", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"e1", ",", "e2"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x1", "[", "t", "]"}], ",", 
      RowBox[{"x2", "[", "t", "]"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6504079239145813`*^9, 3.650407929867922*^9}, {
  3.6504080304846764`*^9, 3.6504080458125534`*^9}, {3.650408107307068*^9, 
  3.6504081075470815`*^9}, {3.650408251636323*^9, 3.650408285138239*^9}, {
  3.6504087418133545`*^9, 3.650408742758409*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"x1", "[", "t", "]"}], "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"m2", " ", 
         RowBox[{"r", "[", "t", "]"}]}], "-", 
        RowBox[{"m1", " ", 
         RowBox[{"X", "[", "t", "]"}]}], "-", 
        RowBox[{"m2", " ", 
         RowBox[{"X", "[", "t", "]"}]}]}], 
       RowBox[{"m1", "+", "m2"}]]}]}], ",", 
    RowBox[{
     RowBox[{"x2", "[", "t", "]"}], "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"-", "m1"}], " ", 
         RowBox[{"r", "[", "t", "]"}]}], "-", 
        RowBox[{"m1", " ", 
         RowBox[{"X", "[", "t", "]"}]}], "-", 
        RowBox[{"m2", " ", 
         RowBox[{"X", "[", "t", "]"}]}]}], 
       RowBox[{"m1", "+", "m2"}]]}]}]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{
  3.6504080461995754`*^9, 3.6504081079301033`*^9, 3.6504081586740055`*^9, {
   3.650408270185384*^9, 3.6504082855832644`*^9}, {3.650408737412103*^9, 
   3.650408744146488*^9}, 3.6504110463821464`*^9, 3.65045490548697*^9, 
   3.6504590367596383`*^9, 3.650459475067664*^9}]
}, Open  ]],

Cell["Position of first body in terms of r and X is given by:", "Text",
 CellChangeTimes->{{3.650414458637306*^9, 3.65041448667791*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"x1", "[", "t", "]"}], "=", 
  RowBox[{
   RowBox[{"x1", "[", "t", "]"}], "/.", 
   RowBox[{"s", "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.6504080758912735`*^9, 3.6504080781634035`*^9}, {
  3.650408110947276*^9, 3.650408117243636*^9}, {3.650408292241645*^9, 
  3.650408294929799*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{
    RowBox[{"m2", " ", 
     RowBox[{"r", "[", "t", "]"}]}], "-", 
    RowBox[{"m1", " ", 
     RowBox[{"X", "[", "t", "]"}]}], "-", 
    RowBox[{"m2", " ", 
     RowBox[{"X", "[", "t", "]"}]}]}], 
   RowBox[{"m1", "+", "m2"}]]}]], "Output",
 CellChangeTimes->{3.6504081178926735`*^9, 3.6504081610321407`*^9, 
  3.6504082953348227`*^9, 3.6504087474726787`*^9, 3.6504110494913244`*^9, 
  3.6504549082087765`*^9, 3.650459039383046*^9, 3.6504594750832644`*^9}]
}, Open  ]],

Cell["Position of second body in terms of r and X is given by:", "Text",
 CellChangeTimes->{{3.650414494421353*^9, 3.650414497644537*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"x2", "[", "t", "]"}], "=", 
  RowBox[{
   RowBox[{"x2", "[", "t", "]"}], "/.", 
   RowBox[{"s", "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.650408120965849*^9, 3.65040813829184*^9}, {
  3.6504082974899454`*^9, 3.650408299690071*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", "m1"}], " ", 
     RowBox[{"r", "[", "t", "]"}]}], "-", 
    RowBox[{"m1", " ", 
     RowBox[{"X", "[", "t", "]"}]}], "-", 
    RowBox[{"m2", " ", 
     RowBox[{"X", "[", "t", "]"}]}]}], 
   RowBox[{"m1", "+", "m2"}]]}]], "Output",
 CellChangeTimes->{{3.6504081291043143`*^9, 3.650408163202265*^9}, 
   3.650408300124096*^9, 3.650408751044883*^9, 3.650411052837516*^9, 
   3.6504549110363817`*^9, 3.6504590482518625`*^9, 3.6504594750832644`*^9}]
}, Open  ]],

Cell["The kinetic energy KE is then given by:", "Text",
 CellChangeTimes->{{3.6504145113913236`*^9, 3.6504145199248114`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"KE", "=", 
  RowBox[{"Simplify", "[", 
   RowBox[{
    RowBox[{"1", "/", "2"}], "  ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"m1", " ", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"x1", "[", "t", "]"}], ",", "t"}], "]"}], "^", "2"}]}], " ",
       "+", "  ", 
      RowBox[{"m2", " ", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"x2", "[", "t", "]"}], ",", "t"}], "]"}], "^", "2"}]}]}], 
     ")"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.650408184418478*^9, 3.650408201970482*^9}, {
   3.6504083067134733`*^9, 3.6504083906172705`*^9}, {3.6504084279534063`*^9, 
   3.6504084318406286`*^9}, {3.6504086665280485`*^9, 3.6504086671740856`*^9}, 
   3.650408756029168*^9}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"m1", " ", "m2", " ", 
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["r", "\[Prime]",
       MultilineFunction->None], "[", "t", "]"}], "2"]}], "+", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"m1", "+", "m2"}], ")"}], "2"], " ", 
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["X", "\[Prime]",
       MultilineFunction->None], "[", "t", "]"}], "2"]}]}], 
  RowBox[{"2", " ", 
   RowBox[{"(", 
    RowBox[{"m1", "+", "m2"}], ")"}]}]]], "Output",
 CellChangeTimes->{{3.6504083252875357`*^9, 3.650408359891513*^9}, 
   3.6504083955115504`*^9, 3.6504084324666643`*^9, 3.6504086675221057`*^9, 
   3.6504087566532035`*^9, 3.6504088936280384`*^9, 3.65041105483563*^9, 
   3.6504549127543845`*^9, 3.6504590516888685`*^9, 3.650459475114464*^9}]
}, Open  ]],

Cell["Since X\[CloseCurlyQuote][t] = Vcm, we can write KE as:", "Text",
 CellChangeTimes->{{3.650408583525304*^9, 3.6504086008212934`*^9}, 
   3.6504145302924047`*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"KE", "=", 
  RowBox[{"KE", "/.", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"X", "'"}], "[", "t", "]"}], "\[Rule]", 
     RowBox[{"Vcm", "[", "t", "]"}]}], "}"}]}]}]], "Input",
 CellChangeTimes->{{3.65040860495453*^9, 3.650408632614112*^9}, {
  3.650408677054651*^9, 3.6504086838220377`*^9}, {3.6504088789481983`*^9, 
  3.650408879884252*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"m1", "+", "m2"}], ")"}], "2"], " ", 
    SuperscriptBox[
     RowBox[{"Vcm", "[", "t", "]"}], "2"]}], "+", 
   RowBox[{"m1", " ", "m2", " ", 
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["r", "\[Prime]",
       MultilineFunction->None], "[", "t", "]"}], "2"]}]}], 
  RowBox[{"2", " ", 
   RowBox[{"(", 
    RowBox[{"m1", "+", "m2"}], ")"}]}]]], "Output",
 CellChangeTimes->{{3.6504086195133624`*^9, 3.6504086330301356`*^9}, {
   3.650408679055765*^9, 3.650408685109112*^9}, {3.6504088804632854`*^9, 
   3.650408896262189*^9}, 3.6504110599479227`*^9, 3.650454914449188*^9, 
   3.650459054294073*^9, 3.650459475114464*^9}]
}, Open  ]],

Cell["\<\
Now in spherical coordinates r is can be written as: (where we have renamed \
for convinience the angles \[Theta] -> A and \[Phi] -> B).\
\>", "Text",
 CellChangeTimes->{{3.6504091580171547`*^9, 3.6504093233506107`*^9}, {
   3.650409409510536*^9, 3.650409439413246*^9}, 3.6504101066234035`*^9, {
   3.650414537063792*^9, 3.650414553364724*^9}, {3.650453166313055*^9, 
   3.6504531802028494`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"r", "[", "t_", "]"}], "=", 
  RowBox[{
   RowBox[{"R", "[", "t", "]"}], 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"Sin", "[", 
       RowBox[{"A", "[", "t", "]"}], "]"}], 
      RowBox[{"Cos", "[", 
       RowBox[{"B", "[", "t", "]"}], "]"}]}], ",", 
     RowBox[{
      RowBox[{"Sin", "[", 
       RowBox[{"A", "[", "t", "]"}], "]"}], 
      RowBox[{"Sin", "[", 
       RowBox[{"B", "[", "t", "]"}], "]"}]}], ",", 
     RowBox[{"Cos", "[", 
      RowBox[{"A", "[", "t", "]"}], "]"}]}], "}"}]}]}]], "Input",
 CellChangeTimes->{
  3.6504092040137854`*^9, {3.6504093104278717`*^9, 3.650409314977132*^9}, {
   3.6504093548644133`*^9, 3.6504093554804487`*^9}, {3.650409389143371*^9, 
   3.6504094464556494`*^9}, {3.6504094884070487`*^9, 3.650409506600089*^9}, {
   3.650409886651824*^9, 3.6504098870108447`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"Cos", "[", 
     RowBox[{"B", "[", "t", "]"}], "]"}], " ", 
    RowBox[{"R", "[", "t", "]"}], " ", 
    RowBox[{"Sin", "[", 
     RowBox[{"A", "[", "t", "]"}], "]"}]}], ",", 
   RowBox[{
    RowBox[{"R", "[", "t", "]"}], " ", 
    RowBox[{"Sin", "[", 
     RowBox[{"A", "[", "t", "]"}], "]"}], " ", 
    RowBox[{"Sin", "[", 
     RowBox[{"B", "[", "t", "]"}], "]"}]}], ",", 
   RowBox[{
    RowBox[{"Cos", "[", 
     RowBox[{"A", "[", "t", "]"}], "]"}], " ", 
    RowBox[{"R", "[", "t", "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{{3.6504093649269886`*^9, 3.650409404799267*^9}, {
   3.650409442822441*^9, 3.65040944663966*^9}, {3.650409490674178*^9, 
   3.6504095071371202`*^9}, 3.6504098874038672`*^9, 3.650411063678136*^9, {
   3.650454916182791*^9, 3.650454918384395*^9}, 3.6504590569404783`*^9, 
   3.650459475130064*^9}]
}, Open  ]],

Cell["This gives KE in terms of R, A and B as:", "Text",
 CellChangeTimes->{{3.65040945538616*^9, 3.6504094936843505`*^9}, {
  3.650414559292063*^9, 3.6504145604451294`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"KE", "=", 
  RowBox[{"Simplify", "[", 
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"m1", "+", "m2"}], ")"}], "2"], " ", 
      SuperscriptBox[
       RowBox[{"Vcm", "[", "t", "]"}], "2"]}], "+", 
     RowBox[{"m1", " ", "m2", " ", 
      RowBox[{
       RowBox[{
        SuperscriptBox["r", "\[Prime]",
         MultilineFunction->None], "[", "t", "]"}], ".", 
       RowBox[{
        RowBox[{"r", "'"}], "[", "t", "]"}]}]}]}], 
    RowBox[{"2", " ", 
     RowBox[{"(", 
      RowBox[{"m1", "+", "m2"}], ")"}]}]], "]"}]}]], "Input",
 CellChangeTimes->{{3.6504096005964656`*^9, 3.6504096459650574`*^9}, {
  3.650409689067523*^9, 3.6504097107397623`*^9}, {3.650411082370205*^9, 
  3.6504111014792976`*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"m1", "+", "m2"}], ")"}], "2"], " ", 
    SuperscriptBox[
     RowBox[{"Vcm", "[", "t", "]"}], "2"]}], "+", 
   RowBox[{"m1", " ", "m2", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"R", "[", "t", "]"}], "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{
           SuperscriptBox["A", "\[Prime]",
            MultilineFunction->None], "[", "t", "]"}], "2"], "+", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"Sin", "[", 
            RowBox[{"A", "[", "t", "]"}], "]"}], "2"], " ", 
          SuperscriptBox[
           RowBox[{
            SuperscriptBox["B", "\[Prime]",
             MultilineFunction->None], "[", "t", "]"}], "2"]}]}], ")"}]}], 
      "+", 
      SuperscriptBox[
       RowBox[{
        SuperscriptBox["R", "\[Prime]",
         MultilineFunction->None], "[", "t", "]"}], "2"]}], ")"}]}]}], 
  RowBox[{"2", " ", 
   RowBox[{"(", 
    RowBox[{"m1", "+", "m2"}], ")"}]}]]], "Output",
 CellChangeTimes->{{3.650409700015149*^9, 3.6504097111597867`*^9}, 
   3.650409890510045*^9, 3.6504111044874697`*^9, 3.650454920429999*^9, 
   3.6504590607644854`*^9, 3.6504594753016644`*^9}]
}, Open  ]],

Cell["Potential energy (PE) is given by:", "Text",
 CellChangeTimes->{{3.650410117054*^9, 3.6504101251184616`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PE", "=", 
  RowBox[{
   RowBox[{"-", "G"}], " ", "m1", " ", 
   RowBox[{"m2", " ", "/", " ", 
    RowBox[{"R", "[", "t", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.650409919409698*^9, 3.6504099396258545`*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"G", " ", "m1", " ", "m2"}], 
   RowBox[{"R", "[", "t", "]"}]]}]], "Output",
 CellChangeTimes->{3.650409945429186*^9, 3.6504111071896243`*^9, 
  3.650454922130402*^9, 3.6504590641672916`*^9, 3.6504594753016644`*^9}]
}, Open  ]],

Cell["Thus the total hamiltonian in terms of R, A and B is:", "Text",
 CellChangeTimes->{{3.650409951744547*^9, 3.6504099612640915`*^9}, 
   3.650410109326558*^9, {3.650453317615709*^9, 3.6504533235750494`*^9}, 
   3.650453399214376*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"H", "=", 
  RowBox[{"KE", "+", "PE"}]}]], "Input",
 CellChangeTimes->{{3.6504099647022886`*^9, 3.6504099689695325`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"G", " ", "m1", " ", "m2"}], 
    RowBox[{"R", "[", "t", "]"}]]}], "+", 
  FractionBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"m1", "+", "m2"}], ")"}], "2"], " ", 
     SuperscriptBox[
      RowBox[{"Vcm", "[", "t", "]"}], "2"]}], "+", 
    RowBox[{"m1", " ", "m2", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"R", "[", "t", "]"}], "2"], " ", 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           RowBox[{
            SuperscriptBox["A", "\[Prime]",
             MultilineFunction->None], "[", "t", "]"}], "2"], "+", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"Sin", "[", 
             RowBox[{"A", "[", "t", "]"}], "]"}], "2"], " ", 
