Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Y", "==", " ", 
  RowBox[{
   RowBox[{"1", "/", "2"}], " ", "C", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{"m1", " ", "m2"}], ")"}], " "}]}]}]], "Input",
 CellChangeTimes->{{3.6504584689182167`*^9, 3.650458480333739*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   FractionBox["1", "2"], " ", 
   SuperscriptBox[
    RowBox[{"R", "[", "t", "]"}], "2"], " ", 
   RowBox[{
    SuperscriptBox["A", "\[Prime]",
     MultilineFunction->None], "[", "t", "]"}]}], "\[Equal]", 
  FractionBox[
   RowBox[{"C", " ", 
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}]}], 
   RowBox[{"2", " ", "m1", " ", "m2"}]]}]], "Output",
 CellChangeTimes->{3.6504591558182716`*^9, 3.650459475410865*^9}]
}, Open  ]],

Cell[TextData[StyleBox["Thus Y is a constant. Kepler's second law is \
obeyed.", "Subsubsection"]], "Text",
 CellChangeTimes->{{3.6504143255676947`*^9, 3.6504143338631697`*^9}, {
  3.6504146187154617`*^9, 3.65041462882804*^9}, {3.6504174640242043`*^9, 
  3.650417471129611*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["c)", "Subchapter",
 CellChangeTimes->{{3.6504146059317307`*^9, 3.650414612924131*^9}}],

Cell["\<\
Since the motion is confined to a plane, the lagrangian in terms of R and B \
(angle in the plane) is given by:\
\>", "Text",
 CellChangeTimes->{{3.6504551640964656`*^9, 3.6504552040073404`*^9}, {
  3.6504553095840683`*^9, 3.6504553106760697`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"L", "=", 
  RowBox[{"L", "/.", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"A", "'"}], "[", "t", "]"}], "\[Rule]", "0"}], ",", 
     RowBox[{
      RowBox[{"A", "[", "t", "]"}], "\[Rule]", 
      RowBox[{"Pi", "/", "2"}]}]}], "}"}]}]}]], "Input",
 CellChangeTimes->{{3.650454892122346*^9, 3.6504548930115476`*^9}, 
   3.650454943845446*^9, {3.6504553140450764`*^9, 3.650455323487895*^9}, {
   3.650455361791769*^9, 3.650455362088169*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{"G", " ", "m1", " ", "m2"}], 
   RowBox[{"R", "[", "t", "]"}]], "+", 
  FractionBox[
   RowBox[{"m1", " ", "m2", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"R", "[", "t", "]"}], "2"], " ", 
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["B", "\[Prime]",
          MultilineFunction->None], "[", "t", "]"}], "2"]}], "+", 
      SuperscriptBox[
       RowBox[{
        SuperscriptBox["R", "\[Prime]",
         MultilineFunction->None], "[", "t", "]"}], "2"]}], ")"}]}], 
   RowBox[{"2", " ", 
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}]}]]}]], "Output",
 CellChangeTimes->{
  3.650454893666749*^9, {3.6504549386622367`*^9, 3.6504549609726768`*^9}, 
   3.6504553237842956`*^9, 3.6504553627121706`*^9, 3.650459176694311*^9, 
   3.650459475410865*^9}]
}, Open  ]],

Cell["The angular momentum J is given by:", "Text",
 CellChangeTimes->{{3.650455378427601*^9, 3.6504553898220224`*^9}, {
  3.6504568191341543`*^9, 3.650456819539755*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"J", "==", 
  RowBox[{"D", "[", 
   RowBox[{"L", ",", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{"B", "[", "t", "]"}], ",", "t"}], "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.650455393347029*^9, 3.65045539376823*^9}, {
  3.650456815754548*^9, 3.6504568224297605`*^9}}],

Cell[BoxData[
 RowBox[{"J", "\[Equal]", 
  FractionBox[
   RowBox[{"m1", " ", "m2", " ", 
    SuperscriptBox[
     RowBox[{"R", "[", "t", "]"}], "2"], " ", 
    RowBox[{
     SuperscriptBox["B", "\[Prime]",
      MultilineFunction->None], "[", "t", "]"}]}], 
   RowBox[{"m1", "+", "m2"}]]}]], "Output",
 CellChangeTimes->{{3.650455365136175*^9, 3.650455394550232*^9}, 
   3.6504568229611616`*^9, 3.650459179551116*^9, 3.6504594754264646`*^9}]
}, Open  ]],

Cell["Let u be the reduced mass of the system:", "Text",
 CellChangeTimes->{{3.650455603672832*^9, 3.650455617647857*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"u", "\[Equal]", 
  RowBox[{"m1", " ", 
   RowBox[{"m2", "/", " ", 
    RowBox[{"(", 
     RowBox[{"m1", "+", "m2"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.65045561908506*^9, 3.650455631330284*^9}}],

Cell[BoxData[
 RowBox[{"u", "\[Equal]", 
  FractionBox[
   RowBox[{"m1", " ", "m2"}], 
   RowBox[{"m1", "+", "m2"}]]}]], "Output",
 CellChangeTimes->{3.6504591815635195`*^9, 3.6504594754264646`*^9}]
}, Open  ]],

Cell["Let us define the constant k as:", "Text",
 CellChangeTimes->{{3.6504591986403513`*^9, 3.6504592129573784`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"k", "\[Equal]", " ", 
  RowBox[{"G", " ", "m1", " ", "m2"}]}]], "Input",
 CellChangeTimes->{{3.6504592308836117`*^9, 3.6504592327746162`*^9}}],

Cell[BoxData[
 RowBox[{"k", "\[Equal]", 
  RowBox[{"G", " ", "m1", " ", "m2"}]}]], "Output",
 CellChangeTimes->{3.6504592333724174`*^9, 3.6504594754420643`*^9}]
}, Open  ]],

Cell["This gives the total energy En as:", "Text",
 CellChangeTimes->{{3.6504555568763404`*^9, 3.650455565211958*^9}, 
   3.6504562895979443`*^9, {3.6504580725634685`*^9, 3.650458072781869*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"En", "\[Equal]", 
  RowBox[{
   RowBox[{
    RowBox[{"1", "/", "2"}], " ", "u", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"R", "'"}], "[", "t", "]"}], "^", "2"}]}], " ", "+", " ", 
   RowBox[{
    RowBox[{"J", "^", "2"}], "/", 
    RowBox[{"(", 
     RowBox[{"2", " ", "u", " ", 
      RowBox[{
       RowBox[{"R", "[", "t", "]"}], "^", "2"}]}], ")"}]}], "-", 
   RowBox[{"k", "/", 
    RowBox[{"R", "[", "t", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.6504555678823643`*^9, 3.6504555938394136`*^9}, {
   3.650455637453495*^9, 3.650455655675129*^9}, {3.6504562401742516`*^9, 
   3.6504562918609486`*^9}, 3.6504568307847757`*^9, {3.6504580745788717`*^9, 
   3.650458074750472*^9}}],

Cell[BoxData[
 RowBox[{"En", "\[Equal]", 
  RowBox[{
   FractionBox[
    SuperscriptBox["J", "2"], 
    RowBox[{"2", " ", "u", " ", 
     SuperscriptBox[
      RowBox[{"R", "[", "t", "]"}], "2"]}]], "-", 
   FractionBox["k", 
    RowBox[{"R", "[", "t", "]"}]], "+", 
   RowBox[{
    FractionBox["1", "2"], " ", "u", " ", 
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["R", "\[Prime]",
       MultilineFunction->None], "[", "t", "]"}], "2"]}]}]}]], "Output",
 CellChangeTimes->{{3.6504562762953196`*^9, 3.65045629260975*^9}, 
   3.650456833039383*^9, 3.650458082387287*^9, 3.6504592398076296`*^9, 
   3.6504594754420643`*^9}]
}, Open  ]],

Cell[TextData[{
 "This gives the change in angle B as R goes from ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", 
    RowBox[{"min", " "}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["R", "B"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (R at angle B) as:"
}], "Text",
 CellChangeTimes->{{3.650456502590149*^9, 3.6504565170781775`*^9}, {
   3.6504565608080635`*^9, 3.6504566035013423`*^9}, 3.6504572020020475`*^9}],

Cell[BoxData[
 RowBox[{"B", "==", 
  RowBox[{
   RowBox[{"J", "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Sqrt", "[", "2", "]"}], " ", "u"}], ")"}]}], " ", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"R", "^", "2"}], " ", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"En", "+", " ", 
          RowBox[{"k", "/", "R"}], "-", 
          RowBox[{
           RowBox[{"J", "^", "2"}], " ", "/", 
           RowBox[{"(", 
            RowBox[{"2", " ", "u", " ", 
             RowBox[{"R", "^", "2"}]}], ")"}]}]}], "]"}]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"R", ",", 
       SubscriptBox["R", "min"], ",", 
       SubscriptBox["R", "B"]}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.6504566225351777`*^9, 3.6504567949993086`*^9}, {
   3.6504568417883987`*^9, 3.650456894610695*^9}, {3.65045704473108*^9, 
   3.650457048116286*^9}, {3.6504570937123938`*^9, 3.6504570938683944`*^9}, {
   3.650457126464698*^9, 3.65045715375315*^9}, {3.6504572062188573`*^9, 
   3.6504572176168776`*^9}, 3.6504573720143795`*^9, {3.650458079332281*^9, 
   3.6504580794590816`*^9}}],

Cell["Calculating the integral gives:", "Text",
 CellChangeTimes->{{3.650457414489465*^9, 3.6504574210750775`*^9}, {
  3.6504592491814485`*^9, 3.650459254005857*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"1", "/", "R"}], " ", "==", " ", 
  RowBox[{
   RowBox[{"u", " ", 
    RowBox[{
     RowBox[{"k", "^", "2"}], "/", 
     RowBox[{"J", "^", "2"}]}]}], "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"1", "/", 
       SubscriptBox["R", "Min"]}], "-", 
      RowBox[{"u", " ", 
       RowBox[{"k", "/", 
        RowBox[{"J", "^", "2"}]}]}]}], ")"}], " ", 
    RowBox[{"Cos", "[", "B", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.6504574244954844`*^9, 3.650457485147998*^9}, 
   3.6504592607842693`*^9}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "R"], "\[Equal]", 
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["k", "2"], " ", "u"}], 
    SuperscriptBox["J", "2"]], "+", 
   RowBox[{
    RowBox[{"Cos", "[", "B", "]"}], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{"k", " ", "u"}], 
        SuperscriptBox["J", "2"]]}], "+", 
      FractionBox["1", 
       SubscriptBox["R", "Min"]]}], ")"}]}]}]}]], "Output",
 CellChangeTimes->{{3.650459256969862*^9, 3.6504592613770704`*^9}, 
   3.6504594970413046`*^9}]
}, Open  ]],

Cell["\<\
This is the equation of a conic section which is an ellipse for e < 1\
\>", "Text",
 CellChangeTimes->{{3.650457494761016*^9, 3.6504575010538273`*^9}, {
  3.650457653775518*^9, 3.650457668944146*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"R", "[", "B", "]"}], "\[Equal]", " ", 
  RowBox[{"p", "/", 
   RowBox[{"(", 
    RowBox[{"1", "+", " ", 
     RowBox[{"e", " ", 
      RowBox[{"Cos", "[", "B", "]"}]}]}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.6504575113328485`*^9, 3.6504575394333005`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"R", "[", "B", "]"}], "\[Equal]", 
  FractionBox["p", 
   RowBox[{"1", "+", 
    RowBox[{"e", " ", 
     RowBox[{"Cos", "[", "B", "]"}]}]}]]}]], "Output",
 CellChangeTimes->{3.650459264134077*^9, 3.6504594972451053`*^9}]
}, Open  ]],

Cell["\<\
where p is the latus rectum and e is the eccentricity which have the values:\
\>", "Text",
 CellChangeTimes->{{3.650457544957111*^9, 3.650457571725361*^9}, {
  3.650459271068489*^9, 3.6504592882279205`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"p", "=", " ", 
  RowBox[{
   RowBox[{"J", "^", "2"}], "/", 
   RowBox[{"(", 
    RowBox[{"u", " ", "k"}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.650457551343522*^9, 3.650457588418792*^9}, {
  3.650457695156998*^9, 3.6504576966410007`*^9}}],

Cell[BoxData[
 FractionBox[
  SuperscriptBox["J", "2"], 
  RowBox[{"k", " ", "u"}]]], "Output",
 CellChangeTimes->{3.650457703400813*^9, 3.650458102686324*^9, 
  3.6504592913433266`*^9, 3.650459497260705*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"e", "=", " ", 
  RowBox[{"Sqrt", "[", 
   RowBox[{"1", " ", "+", " ", 
    RowBox[{"2", "En", " ", 
     RowBox[{
      RowBox[{"J", "^", "2"}], "/", 
      RowBox[{"(", 
       RowBox[{"m", " ", 
        RowBox[{"k", "^", "2"}]}], ")"}]}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6504575931410007`*^9, 3.6504576241582603`*^9}, {
  3.650457700154007*^9, 3.65045770146441*^9}, {3.6504580924563055`*^9, 
  3.6504580927215056`*^9}}],

Cell[BoxData[
 SqrtBox[
  RowBox[{"1", "+", 
   FractionBox[
    RowBox[{"2", " ", "En", " ", 
     SuperscriptBox["J", "2"]}], 
    RowBox[{
     SuperscriptBox["k", "2"], " ", "m"}]]}]]], "Output",
 CellChangeTimes->{3.6504577054756165`*^9, 3.6504580954847107`*^9, 
  3.650459292778529*^9, 3.650459497260705*^9}]
}, Open  ]],

Cell["The semi major axis is then given by", "Text",
 CellChangeTimes->{{3.650457707741621*^9, 3.650457716550438*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"a", "=", " ", 
  RowBox[{"p", "/", 
   RowBox[{"(", 
    RowBox[{"1", "-", 
     RowBox[{"e", "^", "2"}]}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.6504577197660437`*^9, 3.6504577299522624`*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"k", " ", "m"}], 
   RowBox[{"2", " ", "En", " ", "u"}]]}]], "Output",
 CellChangeTimes->{
  3.650457730794664*^9, {3.6504580998265185`*^9, 3.650458104408327*^9}, 
   3.6504592969391365`*^9, 3.650459497276305*^9}]
}, Open  ]],

Cell["Area of the ellipse is given by:", "Text",
 CellChangeTimes->{{3.65045786868224*^9, 3.65045788433327*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"area", " ", "=", " ", 
  RowBox[{
   RowBox[{"Pi", " ", 
    RowBox[{"a", "^", "2"}], " ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"e", "^", "2"}]}], ")"}], "]"}]}], "//", 
   "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.650457886393474*^9, 3.6504579208403645`*^9}, {
  3.6504580491726217`*^9, 3.650458050734625*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["k", "2"], " ", 
   SqrtBox[
    RowBox[{"-", 
     FractionBox[
      RowBox[{"En", " ", 
       SuperscriptBox["J", "2"]}], 
      RowBox[{
       SuperscriptBox["k", "2"], " ", "m"}]]}]], " ", 
   SuperscriptBox["m", "2"], " ", "\[Pi]"}], 
  RowBox[{"2", " ", 
   SqrtBox["2"], " ", 
   SuperscriptBox["En", "2"], " ", 
   SuperscriptBox["u", "2"]}]]], "Output",
 CellChangeTimes->{3.650457928308381*^9, 3.650458051259426*^9, 
  3.650458106708933*^9, 3.6504592992167406`*^9, 3.6504594972919054`*^9}]
}, Open  ]],

Cell["\<\
Time taken to complete one full orbit (T) is equal to area divided by rate of \
change of area swept per unit time (Y)\
\>", "Text",
 CellChangeTimes->{{3.6504581152751493`*^9, 3.6504581468116074`*^9}, {
  3.6504585781740656`*^9, 3.650458579475498*^9}, {3.650458641536559*^9, 
  3.650458642307172*^9}}],

Cell["Y is given by:", "Text",
 CellChangeTimes->{{3.6504593078067565`*^9, 3.650459324258587*^9}}],

Cell[CellGroupData[{

Cell[BoxData["Y"], "Input",
 CellChangeTimes->{{3.6504585821097474`*^9, 3.650458590715209*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], " ", 
  SuperscriptBox[
   RowBox[{"R", "[", "t", "]"}], "2"], " ", 
  RowBox[{
   SuperscriptBox["A", "\[Prime]",
    MultilineFunction->None], "[", "t", "]"}]}]], "Output",
 CellChangeTimes->{3.650458591168424*^9, 3.650459497307505*^9}]
}, Open  ]],

Cell["Angular momentum J was shown to be:", "Text",
 CellChangeTimes->{{3.650459331785201*^9, 3.6504593487478323`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"J", "\[Equal]", 
  RowBox[{"u", " ", 
   RowBox[{
    RowBox[{"R", "[", "t", "]"}], "^", "2"}], " ", 
   RowBox[{
    RowBox[{"A", "'"}], "[", "t", "]"}]}]}]], "Input",
 CellChangeTimes->{{3.650458543982622*^9, 3.6504585680210123`*^9}}],

Cell[BoxData[
 RowBox[{"J", "\[Equal]", 
  RowBox[{"u", " ", 
   SuperscriptBox[
    RowBox[{"R", "[", "t", "]"}], "2"], " ", 
   RowBox[{
    SuperscriptBox["A", "\[Prime]",
     MultilineFunction->None], "[", "t", "]"}]}]}]], "Output",
 CellChangeTimes->{3.650459351098837*^9, 3.650459497307505*^9}]
}, Open  ]],

Cell["Therefore", "Text",
 CellChangeTimes->{{3.6504586018938637`*^9, 3.650458603291112*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Y", "=", 
  RowBox[{"J", "/", 
   RowBox[{"(", 
    RowBox[{"2", "m"}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.6504586049189463`*^9, 3.650458620588332*^9}}],

Cell[BoxData[
 FractionBox["J", 
  RowBox[{"2", " ", "m"}]]], "Output",
 CellChangeTimes->{3.650458633439972*^9, 3.6504593543748426`*^9, 
  3.650459497323105*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"T", "=", " ", 
  RowBox[{
   RowBox[{"area", "/", "Y"}], " ", "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.650458361609468*^9, 3.6504584048181553`*^9}, {
  3.650458670172202*^9, 3.650458687899619*^9}, {3.6504593606560545`*^9, 
  3.650459372560276*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["J", "3"], " ", "m", " ", "\[Pi]"}], 
  RowBox[{
   SqrtBox["2"], " ", 
   SuperscriptBox["k", "2"], " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"En", " ", 
        SuperscriptBox["J", "2"]}], 
       RowBox[{
        SuperscriptBox["k", "2"], " ", "m"}]]}], ")"}], 
    RowBox[{"3", "/", "2"}]], " ", 
   SuperscriptBox["u", "2"]}]]], "Output",
 CellChangeTimes->{
  3.6504584056801577`*^9, {3.65045866240281*^9, 3.6504586882362247`*^9}, {
   3.650459357237648*^9, 3.650459372950277*^9}, 3.650459497338705*^9}]
}, Open  ]],

Cell[TextData[StyleBox["Thus we get T is proportional to En^(-3/2). We have \
shown previously that the semi major axis a is inversely proportional to En. \
Therefore we get Kepler\[CloseCurlyQuote]s third law i.e T^2 proportional to \
a^3.", "Subsubsection"]], "Text",
 CellChangeTimes->{{3.6504586980962443`*^9, 3.6504587984990625`*^9}}]
}, Open  ]]
