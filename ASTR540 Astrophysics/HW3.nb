Cell[TextData[StyleBox["oP is the orbital period, M is the total mass, mP and \
mS are the masses of the primary and secondary, x is the ratio mP / M, dS and \
dP are the distances to the center of mass, vS and vP are the actual \
velocities, whereas vSr and vPr are the observed radial velocities.",
 FontWeight->"Bold"]], "Text",
 CellChangeTimes->{{3.682787941814228*^9, 3.682788083090123*^9}, {
  3.682788710371979*^9, 3.682788785869862*^9}, {3.682788984142753*^9, 
  3.682789007914981*^9}, {3.682790338996506*^9, 3.6827903395830965`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"eqs", "=", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"2", "Pi", " ", 
        RowBox[{"dS", " ", "/", "oP"}]}], "\[Equal]", "vS"}], " ", 
      RowBox[{"(*", 
       RowBox[{"Time", " ", "period", " ", "of", " ", "secondary"}], "*)"}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"2", "Pi", " ", 
        RowBox[{"dP", "/", "oP"}]}], "\[Equal]", "vP"}], " ", 
      RowBox[{"(*", 
       RowBox[{"Time", " ", "period", " ", "of", " ", "primary"}], "*)"}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"vS", " ", "mS"}], "\[Equal]", 
       RowBox[{"mP", " ", "vP"}]}], " ", 
      RowBox[{"(*", 
       RowBox[{"Conservation", " ", "of", " ", "momentum"}], "*)"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"mP", "*", 
        RowBox[{
         RowBox[{"vP", "^", "2"}], "/", "dP"}]}], "\[Equal]", 
       RowBox[{
        TemplateBox[{
         InterpretationBox["\[InvisibleSpace]", 1],StyleBox[
          "\"G\"", Italic, StripOnInput -> False],
          "Newtonian gravitational constants","\"GravitationalConstant\""},
         "Quantity"], " ", "mS", " ", 
        RowBox[{"mP", "/", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"dP", "+", "dS"}], ")"}], "^", "2"}]}]}]}]}], " ", 
     RowBox[{"(*", 
      RowBox[{"Centripetal", " ", "force", " ", "equals", " ", "gravity"}], 
      "*)"}], "\[IndentingNewLine]", "}"}], "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"vS", "->", 
       RowBox[{"vSr", "/", 
        RowBox[{"Sin", "@", "i"}]}]}], ",", 
      RowBox[{"vP", "->", 
       RowBox[{"vPr", "/", 
        RowBox[{"Sin", "@", "i"}]}]}], ",", 
      RowBox[{"mP", "\[Rule]", 
       RowBox[{"x", " ", "M"}]}], ",", " ", 
      RowBox[{"mS", "\[Rule]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "x"}], ")"}], "M"}]}]}], "}"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6827877117440405`*^9, 3.6827877723984594`*^9}, {
