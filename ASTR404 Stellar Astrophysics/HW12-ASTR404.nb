Cell["\<\
Finding dimensionless combinations of the moment of inertia, radius and other \
constants:\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"const", "=", 
  RowBox[{"First", "@", 
   RowBox[{"DimensionalCombinations", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\"\<MomentOfInertia\>\"", ",", "\"\<Radius\>\""}], "}"}], ",", 
     RowBox[{"IncludeQuantities", "->", 
      RowBox[{"{", 
       RowBox[{"\"\<GravitationalConstant\>\"", ",", "\"\<SpeedOfLight\>\""}],
        "}"}]}]}], "]"}]}]}]], "Input"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"(", 
    TemplateBox[{"1",RowBox[{
        SuperscriptBox[
         StyleBox["\"c\"", Italic, StripOnInput -> False], 
         RowBox[{"2", "/", "3"}]], "\[NegativeMediumSpace]", "\"/\"", 
        "\[InvisibleSpace]", 
        RadicalBox[
         StyleBox["\"G\"", Italic, StripOnInput -> False], "3"]}],
      "speed of light in vacuum to the two thirds per cube root Newtonian \
gravitational constant",FractionBox[
       SuperscriptBox["\"SpeedOfLight\"", 
        RowBox[{"2", "/", "3"}]], 
       SuperscriptBox["\"GravitationalConstant\"", 
        RowBox[{"1", "/", "3"}]]]},
     "Quantity"], ")"}], " ", 
   TemplateBox[{"\"Radius\"","\"Radius\""},
    "QuantityVariable",
    DisplayFunction->(TooltipBox[
      StyleBox[#, FontColor -> GrayLevel[0.4], ShowStringCharacters -> False], 
      RowBox[{"Quantity variable:", " radius"}], TooltipStyle -> 
      "TextStyling"]& )]}], 
  SuperscriptBox[
   TemplateBox[{"\"MomentOfInertia\"","\"MomentOfInertia\""},
    "QuantityVariable",
    DisplayFunction->(TooltipBox[
      StyleBox[#, FontColor -> GrayLevel[0.4], ShowStringCharacters -> False], 
      RowBox[{"Quantity variable:", " moment of inertia"}], TooltipStyle -> 
      "TextStyling"]& )], 
   RowBox[{"1", "/", "3"}]]]], "Output"]
}, Open  ]],

Cell["\<\
We can obtain units of \[Omega] by cubing the reciprocal of the above and \
multiplying with \[Omega].\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[Omega]", "/", 
  RowBox[{"const", "^", "3"}]}]], "Input"],

Cell[BoxData[
 FractionBox[
  RowBox[{"\[Omega]", " ", 
   RowBox[{"(", 
    TemplateBox[{"1",RowBox[{
        StyleBox["\"G\"", Italic, StripOnInput -> False], "\[InvisibleSpace]",
         "\"/\"", "\[InvisibleSpace]", 
        SuperscriptBox[
         StyleBox["\"c\"", Italic, StripOnInput -> False], "2"]}],
      "Newtonian gravitational constants per speed of light in vacuum \
squared",FractionBox["\"GravitationalConstant\"", 
       SuperscriptBox["\"SpeedOfLight\"", "2"]]},
     "Quantity"], ")"}], " ", 
   TemplateBox[{"\"MomentOfInertia\"","\"MomentOfInertia\""},
    "QuantityVariable",
    DisplayFunction->(TooltipBox[
      StyleBox[#, FontColor -> GrayLevel[0.4], ShowStringCharacters -> False], 
      RowBox[{"Quantity variable:", " moment of inertia"}], TooltipStyle -> 
      "TextStyling"]& )]}], 
  SuperscriptBox[
   TemplateBox[{"\"Radius\"","\"Radius\""},
    "QuantityVariable",
    DisplayFunction->(TooltipBox[
      StyleBox[#, FontColor -> GrayLevel[0.4], ShowStringCharacters -> False], 
      RowBox[{"Quantity variable:", " radius"}], TooltipStyle -> 
      "TextStyling"]& )], "3"]]], "Output"]
}, Open  ]],

Cell["\<\
Since J is equal to the moment of inertia times \[Omega], we get the desired \
result.\
\>", "Text"]
}, Open  ]],

